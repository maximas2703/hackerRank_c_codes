#include <stdio.h>
/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/

#include <stdio.h>

/*
 * Function: max_of_four
 * --------------------
 * Returns the greatest of four integers.
 */
int max_of_four(int a, int b, int c, int d) {
    int max = a; // Assume 'a' is the largest initially

    if (b > max) {
        max = b;
    }
    if (c > max) {
        max = c;
    }
    if (d > max) {
        max = d;
    }
    
    return max;
}

int main() {
    int a, b, c, d;
    
    // Reading four integers from input
    if (scanf("%d %d %d %d", &a, &b, &c, &d) == 4) {
        int res = max_of_four(a, b, c, d);
        printf("%d\n", res);
    }
    
    return 0;
}
